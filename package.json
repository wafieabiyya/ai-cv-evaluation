{
  "name": "ai-cv-evaluation",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only src/index.ts",
    "worker": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only ./src/queue/workers/evaluation.worker.ts",
    "migrate:create": "dotenv -e .env -- node-pg-migrate create --migrations-dir src/db/migrations --migration-file-language sql",
    "migrate:up": "dotenv -e .env -- node-pg-migrate up --migrations-dir src/db/migrations",
    "migrate:down": "dotenv -e .env -- node-pg-migrate down --migrations-dir src/db/migrations",
    "migrate:redo": "npm run migrate:down && npm run migrate:up",
    "db:ping": "ts-node-dev --transpile-only scripts/db_ping.ts",
    "rag:backfill": "ts-node-dev -r tsconfig-paths/register --transpile-only scripts/rag_backfill.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "bullmq": "^5.58.9",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "ioredis": "^5.8.0",
    "mammoth": "^1.11.0",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "openai": "^5.23.1",
    "pdf-parse": "^1.1.1",
    "pg": "^8.16.3",
    "swagger-ui-express": "^5.0.1",
    "yamljs": "^0.3.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.5.2",
    "@types/pg": "^8.15.5",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/yamljs": "^0.2.34",
    "dotenv-cli": "^10.0.0",
    "node-pg-migrate": "^8.0.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2"
  }
}
